{
    "root": "./docs",
    "author": "Arvinx",
    "language": "zh-hans",
    "styles": {
        "website": "styles/website.css"
    },
    "plugins": [
        "disqus",
        "-highlight",
        "github",
        "expandable-chapters-small",
        "splitter",
        "-sharing",
        "sharing-plus",
        "ga",
        "donate",
        "local-pagefooter",
        "anchors",
        "anchor-navigation-ex"
    ],
    "pluginsConfig": {
        "theme-default": {
            "showLevel": true
        },
        "disqus": {
            "shortName": "arvinx"
        },
        "github": {
            "url": "https://github.com/arvinxx"
        },
        "ga": {
            "token": "UA-110579858-2"
        },
        "sharing": {
            "douban": false,
            "facebook": false,
            "google": true,
            "hatenaBookmark": false,
            "instapaper": false,
            "line": false,
            "linkedin": true,
            "messenger": false,
            "pocket": false,
            "qq": true,
            "qzone": true,
            "stumbleupon": false,
            "twitter": false,
            "viber": false,
            "vk": false,
            "weibo": true,
            "whatsapp": false,
            "all": ["facebook", "google", "twitter", "instapaper", "pocket"]
        },
        "local-pagefooter": {
            "copyright": "Copyright &copy ArvinX",
            "islocal": true,
            "modify_label": "该文件修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "anchor-navigation-ex": {
            "showLevel": false,
            "associatedWithSummary": true,
            "isRewritePageTitle": false,
            "float": {
                "showLevelIcon": false,
                "level1Icon": "fa fa-hand-o-right",
                "level2Icon": "fa fa-hand-o-right",
                "level3Icon": "fa fa-hand-o-right"
            },
            "pageTop": {
                "showLevelIcon": false,
                "level1Icon": "fa fa-hand-o-right",
                "level2Icon": "fa fa-hand-o-right",
                "level3Icon": "fa fa-hand-o-right"
            }
        },
        "donate": {
            "wechat": "/resources/weixin.jpg",
            "alipay": "/resources/alipay.jpg",
            "title": "如有帮助，赞赏一下鼓励更新哦~",
            "button":"赏",
            "alipayText": "支付宝打赏",
            "wechatText": "微信打赏"
        }
    }
}
